                        },
                        "image": {
                            "xpath": ".//p[contains(.,'PRESENTER')]/following-sibling::p[1]/img/@src",
                            "python": "data.replace('//','')"
                        },
                        "name": {
                            "xpath": ".//p[contains(.,'PRESENTER')]/text()"
